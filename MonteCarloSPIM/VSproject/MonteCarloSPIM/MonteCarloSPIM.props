<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <mcxDir>D:\Code\mcx_custom\mcx</mcxDir>
    <MCfocusingDir>D:\Code\MonteCarloFocus\MonteCarloFocus</MCfocusingDir>
    <mcxSourceDir>$(mcxDir)\src</mcxSourceDir>
    <MCfocusingSourceDir>$(MCfocusingDir)\src</MCfocusingSourceDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="mcxDir">
      <Value>$(mcxDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="MCfocusingDir">
      <Value>$(MCfocusingDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="mcxSourceDir">
      <Value>$(mcxSourceDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="MCfocusingSourceDir">
      <Value>$(MCfocusingSourceDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>